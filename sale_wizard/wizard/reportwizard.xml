<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_sale_order_wizard_form" model="ir.ui.view">
        <field name="name">sale.order.wizard.form</field>
        <field name="model">sale.order.wizard</field>
        <field name="arch" type="xml">
            <form string="Sale Order Wizard">
                <group>
                    <field name="upload_file"/>
                </group>
                <footer>
                    <button name="action_open_excel_reports" type="object" string="Conform" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>
